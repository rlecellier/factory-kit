{
  "name": "factory-kit",
  "version": "0.1.0",
  "description": "Factory pattern implementation for generating test data with Faker, inspired by FactoryBoy",
  "scripts": {
    "build:clean": "rimraf dist",
    "build:code": "tsup-node",
    "build": "run-s build:clean build:code",
    "generate:types": "tsc --project tsconfig.json --emitDeclarationOnly --declarationMap",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:all": "yarn test && yarn build && yarn lint",
    "lint": "eslint --max-warnings 0 .",
    "ts-check": "tsc",
    "prepublishOnly": "npm run build",
    "format": "prettier --cache --write ."
  },
  "keywords": [
    "factory",
    "testing",
    "fake data",
    "fixtures",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rlecellier/factory-kit"
  },
  "license": "MIT",
  "author": "Romain Le Cellier <rlecellier@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-file-progress": "^3.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.5.0",
    "jiti": "^2.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.26.0"
  },
  "packageManager": "yarn@1.22.22"
}
